{
    "name": "mieciekbot",
    "version": "1.13.3",
    "description": "NodeJS Discord Bot",
    "repository": {
        "url": "https://github.com/PoProstuMieciek/MieciekBot"
    },
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "start:dev": "node -r dotenv/config index.js > logs/mieciekbot-`date +%Y%m%d%H%M%S`.log",
        "debug": "node -r dotenv/config --nolazy --inspect-brk=9229 index.js > logs/mieciekbot-`date +%Y%m%d%H%M%S`.log",
        "clean": "rm -r logs/*.log"
    },
    "author": "PoProstuMieciek",
    "dependencies": {
        "@discordjs/opus": "0.3.2",
        "canvas": "2.6.1",
        "discord.js": "11.5.1",
        "emoji-regex": "9.0.0",
        "mongoose": "5.9.9",
        "octonode": "0.9.5",
        "random-number-csprng": "1.0.2",
        "random-puppy": "1.1.0",
        "request": "2.88.2",
        "youtube-search": "1.1.4",
        "ytdl-core": "2.1.0"
    },
    "devDependencies": {
        "dotenv": "^8.2.0"
    }
}
